{
  "name": "@chrislyons-dev/flarelette-jwt",
  "version": "1.12.0",
  "type": "module",
  "description": "Environment-driven JWT authentication for Cloudflare Workers with secret-name indirection",
  "keywords": [
    "jwt",
    "authentication",
    "cloudflare",
    "workers",
    "edge",
    "hs512",
    "eddsa",
    "ed25519",
    "jwks",
    "flarelette"
  ],
  "homepage": "https://github.com/chrislyons-dev/flarelette-jwt-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrislyons-dev/flarelette-jwt-kit.git",
    "directory": "packages/flarelette-jwt-ts"
  },
  "bugs": {
    "url": "https://github.com/chrislyons-dev/flarelette-jwt-kit/issues"
  },
  "author": "Chris Lyons",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./adapters/hono": {
      "types": "./dist/adapters/hono.d.ts",
      "default": "./dist/adapters/hono.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "flarelette-jwt-secret": "dist/cli.js",
    "flarelette-jwt-keygen": "dist/keygen.js"
  },
  "scripts": {
    "prepare": "node prepare.js",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "prepublishOnly": "npm run prepare && npm run build"
  },
  "dependencies": {
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "rimraf": "^6.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
